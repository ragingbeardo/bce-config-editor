{
  "build": {
    "beforeDevCommand": "pnpm start --port 1420",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist/beardos-config-emporium-config-editor"
  },
  "package": {
    "productName": "beardos-config-emporium-config-editor",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "dev.beardo",
      "icon": [
        "icons/icon_32px.png",
        "icons/icon_128px.png",
        "icons/icon_256px.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "Beardo's Config Emporium Editor",
        "width": 900,
        "height": 900
      }
    ]
  }
}
